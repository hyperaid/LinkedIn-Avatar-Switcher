{
    "manifest_version": 2,
    "name": "Profile Picture Changer",
    "version": "1.0",
    "description": "Change LinkedIn profile pictures",
    "permissions": [
      "activeTab",
      "https://www.linkedin.com/*"
    ],
     
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }
